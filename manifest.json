{
  "manifest_version": 3,
  "name": "BOJ Verdict Beautify",
  "description": "BOJ 채점 결과를 더 예쁘게 나타냅니다.",
  "version": "1.0.1",
  "author": "cywohoy",
  "action": {
    "default_icon": "assets/logo-48.png"
  },
  "icons": {
    "48": "assets/logo-48.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/problem.js"],
      "css": ["css/verdict.css"],
      "matches": ["https://www.acmicpc.net/*"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "permissions": [],
  "host_permissions": [
    "https://www.acmicpc.net/*"
  ],
  "browser_specific_settings": {
  },
  "web_accessible_resources": [
    {
      "resources": [],
      "matches": ["<all_urls>"]
    }
  ]
}